{
  "name": "xtool",
  "productName": "xtool",
  "version": "1.0.0",
  "main": "build/main.js",
  "license": "MIT",
  "scripts": {
    "package": "rimraf cachouTool-* && electron-packager . --platform=win32 --arch=x64 --icon=src/app.ico",
    "tsc": "tsc --watch",
    "compile": "tsc",
    "watch": "tsc --watch",
    "copyhtml": "npx ts-node ./ts-node-scripts/copyhtml.ts",
    "start": "npm run-script compile && npm run-script copyhtml && npx electron .",
    "dev": "npm run-script watch & node_modules/.bin/electron ."
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "shelljs": "^0.8.3",
    "ts-node": "^8.4.1"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.0",
    "@types/fs-extra": "8.0.1",
    "@types/jquery": "3.3.31",
    "@types/request": "2.48.3",
    "@types/shelljs": "^0.8.6",
    "cheerio": "1.0.0-rc.3",
    "electron-packager": "14.1.0",
    "fs-extra": "8.1.0",
    "jquery": "^3.2.0",
    "material-design-lite": "^1.3.0",
    "request": "^2.81.0",
    "rimraf": "3.0.0",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  }
}